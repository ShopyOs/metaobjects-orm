{
  "name": "@shopyos/metaobjects-orm",
  "version": "0.5.0",
  "license": "MIT",
  "author": "ShopyOS",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShopyOs/metaobjects-orm.git"
  },
  "bugs": {
    "url": "https://github.com/ShopyOs/metaobjects-orm/issues"
  },
  "main": "dist/index.js",
  "module": "dist/metaobjects-orm.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^18 || ^20"
  },
  "size-limit": [
    {
      "path": "dist/metaobjects-orm.cjs.production.min.js",
      "limit": "60 KB"
    },
    {
      "path": "dist/metaobjects-orm.esm.js",
      "limit": "60 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@tsconfig/recommended": "^1.0.7",
    "dts-cli": "^2.0.5",
    "husky": "^9.1.3",
    "size-limit": "^11.0.2",
    "size-limit-node-esbuild": "^0.3.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@shopify/graphql-client": "^0.10.3",
    "@shopify/shopify-api": "^9.5.1"
  }
}
